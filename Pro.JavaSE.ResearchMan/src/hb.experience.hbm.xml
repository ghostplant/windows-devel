<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cn.edu.njust.steduman.database.Experience" table="experience">
    <id column="id" name="id" type="int"/>
    <property column="personId" name="personId" type="string"/>
    <property column="startDate" name="startDate" type="java.util.Date"/>
    <property column="endDate" name="endDate" type="java.util.Date"/>
    <property column="detail" name="detail" type="string"/>
  </class>
</hibernate-mapping>
