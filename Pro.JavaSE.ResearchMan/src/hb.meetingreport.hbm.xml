<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cn.edu.njust.steduman.database.MeetingReport" table="meetingreport">
    <id column="id" name="id" type="int">
      <generator class="increment"/>
    </id>
    <property column="essayId" name="essayId" type="int"/>
    <property column="meetingId" name="meetingId" type="int"/>
    <property column="teacherId" name="teacherId" type="string"/>
    <property column="reportDate" name="reportDate" type="java.util.Date"/>
  </class>
</hibernate-mapping>
