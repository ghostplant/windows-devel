<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cn.edu.njust.steduman.database.Meeting" table="meeting">
    <id column="id" name="id" type="int">
      <generator class="increment"/>
    </id>
    <property column="totalPeople" name="totalPeople" type="int"/>
    <property column="foreignPeople" name="foreignPeople" type="int"/>
    <property column="holdStartDate" name="holdStartDate" type="java.util.Date"/>
    <property column="holdEndDate" name="holdEndDate" type="java.util.Date"/>
    <property column="name" name="name" type="string"/>
  </class>
</hibernate-mapping>
