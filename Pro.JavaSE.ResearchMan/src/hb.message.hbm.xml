<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cn.edu.njust.steduman.database.Message" table="Message">
    <id column="id" name="id" type="int">
      <generator class="increment"/>
    </id>
    <property column="date" name="date" type="java.util.Date"/>
    <property column="message" name="message" type="string"/>
    <property column="srcTeacherId" name="srcTeacherId" type="string"/>
    <property column="dstTeacherId" name="dstTeacherId" type="string"/>
    <property column="readed" name="readed" type="boolean"/>
  </class>
</hibernate-mapping>
