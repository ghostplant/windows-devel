<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cn.edu.njust.steduman.database.Cultivation" table="cultivation">
    <id column="id" name="id" type="int">
      <generator class="increment"/>
    </id>
    <property column="studentId" name="studentId" type="string"/>
    <property column="essayTitle" name="essayTitle" type="string"/>
    <property column="major" name="major" type="string"/>
    <property column="subject" name="subject" type="string"/>
    <property column="date" name="date" type="java.util.Date"/>
  </class>
</hibernate-mapping>
